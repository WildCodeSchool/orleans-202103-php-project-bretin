<form method="post" action="{{ path('education_delete', {'id': education.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ education.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
