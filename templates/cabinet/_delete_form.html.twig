<form method="post" action="{{ path('cabinet_delete', {'id': office.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce cabinet ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ office.id) }}">
    <button class="btn btn-primary text-light">Supprimer</button>
</form>
