<form method="post" action="{{ path('pic_delete', {'id': picture.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce cabinet ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">
    <button class="btn btn-primary col-3">Supprimer</button>
</form>
